import java.time.Instant;

CREATE TABLE Phrase (
  id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  message TEXT NOT NULL,
  date INTEGER AS Instant NOT NULL
);

selectMostRecent:
SELECT *
FROM Phrase
ORDER BY date DESC
LIMIT 1;

selectAll:
SELECT *
FROM Phrase;

select:
SELECT *
FROM Phrase
WHERE id = ?;

insert:
INSERT OR REPLACE INTO Phrase (
message,
date
)
VALUES (?, ?);
